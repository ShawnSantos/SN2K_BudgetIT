<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.snkk.budgetit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Forum"></activity>
        <activity android:name=".Categories" />
        <activity android:name=".AddTransaction" />
        <activity android:name=".Statistics" />
        <activity android:name=".Dashboard" />
        <activity android:name=".Note"/>
        <activity android:name=".Status"/>
        <activity android:name=".PaymentType"/>
        <activity android:name=".AttachPhoto"/>
        <activity android:name=".Home" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>